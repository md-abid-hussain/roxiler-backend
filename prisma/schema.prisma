generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model SalesData {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  category    String
  dateOfSale  DateTime @db.Date
  description String
  image       String
  price       Float
  sold        Boolean
  title       String
}
